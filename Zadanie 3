#include <iostream>
using namespace std;

int main()
{
	int number;

	cout << "Podaj liczbe" <<endl; cin >> number;
		if (number % 2==0) {
			cout << "Liczba jest parzysta" <<endl;

		}
		
		else {
			cout << "Liczba jest nieparzysta" <<endl;

		}
		if (number == 0) { cout << "Wpisana liczba jest zero" << endl; }

		
			
		 return (0);


}
